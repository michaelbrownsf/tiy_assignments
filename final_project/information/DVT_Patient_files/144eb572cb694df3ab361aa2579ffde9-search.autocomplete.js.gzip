$(document).ready(function(){$(".search-autocomplete").autocomplete({open:function(){$("ul.ui-autocomplete").scrollTop(0)},source:function(n,t){$.ajax({url:"http://search.patient.info/autocomplete",dataType:"jsonp",async:!0,data:{query:n.term},success:function(n){t($.map(n.results,function(n){return{label:n.name}}))}})},minLength:2,select:function(n,t){this.value=t.item.label;var i=$(this).closest("form");return i.is(".autocomplete-selfsubmit")&&i.submit(),!1},messages:{noResults:"",results:function(){}}})})